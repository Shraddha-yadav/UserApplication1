@model IEnumerable<UserApplication.Models.Course>

@{
    ViewBag.Title = "CourseList";
}

<h2>CourseList</h2>

<p>
    @Html.ActionLink(" ", " ")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CourseName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CourseName)
            </td>
            <td>
                @*@Html.ActionLink("Edit", "Edit", new { id = item.CourseId }) |
                    @Html.ActionLink("Details", "Details", new { id = item.CourseId }) |*@
                @Html.ActionLink("Delete", "DeleteCourse", new { id = item.CourseId })
            </td>
        </tr>
    }

</table>
<<p>
    @Html.Partial("_AddSubjectButtonPartial")
    @Html.Partial("_BackToListButtonPartial")
    @Html.Partial("_CreateCourseButtonPartial")
</p>
